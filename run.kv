<Car>:
  canvas:
    PushMatrix
    Rotate:
      axis: 0, 0, 1
      angle: self.angle
      origin: self.center


    Color:
      rgba:0, 1, 1, 1
    Rectangle:
      pos: self.pos
      size: self.size
    PopMatrix

<path>:
  canvas:
    Color:
      rgba: 0, 0, 1, 1
    Rectangle:
      pos:self.pos
      size:self.size
<Co>:
  canvas:
    Color:
      rgba: self.c, 0, 0, 1
    Rectangle:
      pos:self.pos
      size:self.size

<grid>:
  car: vroom
  Widget:
    canvas:
      Color:
        rgba: 1, .9, 1, 1
      Rectangle:
        pos:self.pos
        size:self.size
  Car:
    id: vroom
    size_hint: 0.02, 0.02



<box>:

  canvas:
    Color:
      rgba: 1, 0, 0, 1
    Rectangle:
      pos: self.pos
      size:self.size


<rootclass>:
