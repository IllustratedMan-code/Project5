<Car>:
  canvas:
    PushMatrix
    Rotate:
      axis: 0, 0, 1
      angle: self.angle
      origin: self.center


    Color:
      rgba:0, 1, 0, 1
    Rectangle:
      pos: self.pos
      size: self.size
    PopMatrix

<path>:
  canvas:
    Color:
      rgba: 0, 0, 1, 1
    Rectangle:
      pos:self.pos
      size:self.size
<Co>:
  canvas:
    Color:
      rgba: self.c
    Rectangle:
      pos:self.pos
      size:self.size
  Label:
    text: self.parent.txt
    size: self.parent.size
    pos: self.parent.pos

<grid>:
  car: vroom
  Widget:
    canvas:
      Color:
        rgba: 1, .9, 1, 1
      Rectangle:
        pos:self.pos
        size:self.size
  Car:
    id: vroom
    size_hint: 0.015, 0.04

<Lcd>:
  BoxLayout:
    orientation: 'vertical'
    Label:
      text_size: self.size
      halign: 'left'
      valign: 'center'
      size:self.size
      pos: self.pos
      text: "  A:" + self.parent.parent.A
    Label:
      text_size: self.size
      halign: 'left'
      valign: 'center'
      size: self.size
      pos: self.pos
      text: "  B:" + self.parent.parent.B
    Label:
      text_size: self.size
      halign: 'left'
      valign: 'center'
      size: self.size
      pos: self.pos
      text: "  C:" + self.parent.parent.C

<BarcodeButtonLayout>:
  b1: bar1
  b2: bar2
  b3: bar3
  b4: bar4

  Barcode1:
    id: bar1
  Barcode2:
    id: bar2
  Barcode3:
    id: bar3
  Barcode4:
    id: bar4

<ButtonLayout>:
  boxy: b
  barcodes: bars
  orientation: 'vertical'
  BarcodeButtonLayout:
    id: bars
  Tribox:
    id: b
<Tribox>:
  curbar: c
  tri: t

  Triangulate:
    id:t
  Cbar:
    id:c

<Cbar>:
  Image:
    source: self.parent.id
    pos: self.parent.pos
    size: self.parent.size

<triangulate>:
  Label:
    size: self.parent.size
    pos: self.parent.pos
    text: "ACTIVATE IPS"

<Barcode1>:
  Image:
    source: 'barcode1.png'
    size: self.parent.size
    pos: self.parent.pos

<Barcode2>:
  Image:
    source: 'barcode2.png'
    size: self.parent.size
    pos: self.parent.pos

<Barcode3>:
  Image:
    source: 'barcode3.png'
    size: self.parent.size
    pos: self.parent.pos

<Barcode4>:
  Image:
    source: 'barcode4.png'
    size: self.parent.size
    pos: self.parent.pos

<VisionBox>:
  col: c
  barcodes: b

  Barcodereader:
    id: b

  Co:
    id: c

<Barcodereader>:
  Image:
    source: self.parent.id
    size: self.parent.size
    pos: self.parent.pos

<home>:
  canvas:
    Color:
      rgba: 1, 0, 1, 1
    Rectangle:
      size: self.size
      pos: self.pos



<box>:
  canvas.before:
    PushMatrix
    Rotate:
      axis: 0, 0, 1
      angle: self.angle
      origin: self.center
  canvas.after:
    PopMatrix

  RelativeLayout:
    size: self.parent.size
    pos: self.parent.pos



    Widget:
      size_hint: (0.2, 0.25)
      pos_hint: {'x':0, 'y':0.75}
      canvas:
        Color:
          rgba: self.parent.parent.barcode[3]
        Rectangle:
          pos: self.pos
          size: self.size
    Widget:
      size_hint: (0.2, 0.25)
      pos_hint: {'x':0, 'y':0.5}
      canvas:
        Color:
          rgba: self.parent.parent.barcode[2]
        Rectangle:
          pos: self.pos
          size: self.size
    Widget:
      size_hint: (0.2, 0.25)
      pos_hint: {'x':0, 'y':0.25}
      canvas:
        Color:
          rgba: self.parent.parent.barcode[1]
        Rectangle:
          pos: self.pos
          size: self.size
    Widget:
      size_hint: (0.2, 0.25)
      pos_hint: {'x':0}
      canvas:
        Color:
          rgba: self.parent.parent.barcode[0]
        Rectangle:
          pos: self.pos
          size: self.size
    Widget:
      size_hint: (0.8, 1)
      pos_hint: {'x':0.2}
      canvas:
        Color:
          rgba: 1, 0, self.parent.parent.bc, 1
        Rectangle:
          pos: self.pos
          size:self.size


<rootclass>:
